<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/..\FrameXML\UI.xsd">
	<StatusBar name="LeftTimerFrame" parent="SpellActivationOverlayFrame" orientation="VERTICAL" rotatesTexture="true" hidden="true">
		<BarTexture file="Interface\TARGETINGFRAME\UI-StatusBar" />
		<Anchors>
			<Anchor point="RIGHT" relativePoint="LEFT" />
		</Anchors>
		<Layers>
			<Layer level="BACKGROUND">
				<Texture name="LeftGcdWarning" file="Interface\TARGETINGFRAME\UI-StatusBar" rotation="100">
					<Color r=".9" g="0" b="0" />
					<Anchors>
						<Anchor point="BOTTOM" />
					</Anchors>
				</Texture>
			</Layer>
		</Layers>
	</StatusBar>
